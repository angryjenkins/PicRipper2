<div class="container">
  <h1>  {{data.origin}}</h1>
  <table class="table table-hover table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>title</th>
        <th>image</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <td>{{this.id}}</td>
        <td>{{this.title}}</td>
        <td><img src="{{this.url}}" alt="{{this.origin}} - {{this.id}}" style="width:150px; height:auto;"></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
